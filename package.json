{
  "name": "irmaos-farias",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "cypress run",
    "test:security": "snyk test",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "electron:dev": "concurrently \"vite\" \"vite build --config electron/vite.config.ts -w\" \"electron .\"",
    "electron:build": "vite build && vite build --config electron/vite.config.ts && electron-builder",
    "electron:preview": "vite preview",
    "prepare": "husky install",
    "db:migrate": "supabase db push",
    "db:generate": "supabase gen types typescript --local > src/types/database.ts",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "analyze": "vite-bundle-analyzer",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "docs:generate": "typedoc --out docs src",
    "lighthouse": "lighthouse-ci",
    "security:audit": "npm audit && snyk test",
    "validate": "npm run lint && npm run test && npm run build",
    "deploy:staging": "netlify deploy --build",
    "deploy:prod": "netlify deploy --build --prod",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject ./dist && sentry-cli sourcemaps upload ./dist"
  },
  "dependencies": {
    "@sentry/react": "^7.101.1",
    "@sentry/tracing": "^7.101.1",
    "@supabase/supabase-js": "^2.39.7",
    "@tanstack/react-query": "^5.24.1",
    "@tensorflow/tfjs": "^4.17.0",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.4.1",
    "date-fns": "^3.3.1",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.7",
    "framer-motion": "^11.0.5",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "lucide-react": "^0.344.0",
    "msw": "^2.2.1",
    "openapi-typescript": "^6.7.4",
    "otplib": "^12.0.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-hot-toast": "^2.4.1",
    "react-leaflet": "^4.2.1",
    "react-qr-code": "^2.0.12",
    "simple-peer": "^9.11.1",
    "swagger-ui-react": "^5.11.8",
    "three": "^0.161.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@sentry/cli": "^2.28.6",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/blocks": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-vite": "^7.6.17",
    "@storybook/test": "^7.6.17",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/leaflet": "^1.9.8",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@types/simple-peer": "^9.11.8",
    "@types/three": "^0.161.2",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "cypress": "^13.6.6",
    "electron": "^28.2.3",
    "electron-builder": "^24.12.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^9.0.11",
    "lighthouse-ci": "^1.11.0",
    "lint-staged": "^15.2.2",
    "netlify-cli": "^17.16.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "snyk": "^1.1271.0",
    "storybook": "^7.6.17",
    "supabase": "^1.145.4",
    "tailwindcss": "^3.4.1",
    "typedoc": "^0.25.8",
    "typescript": "^5.3.3",
    "vite": "^5.1.3",
    "vite-bundle-analyzer": "^0.9.2",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}